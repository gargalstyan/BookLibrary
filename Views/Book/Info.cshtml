@using BookLibrary.Data.Entities
@model IEnumerable<Author>
<h1>Authors Info</h1>
<table class="table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>BirthData</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>
                <ul>
                    @foreach (var author in Model)
                    {
                        <li>@author.Name</li>
                        
                    }
                </ul>
            </td>
            <td>
                <ul>
                    @foreach (var author in Model)
                    {
                        <li>@author.BirthData</li>
                        
                    }
                </ul>
            </td>
            @* @foreach (var item in Model)
            {
            <td>
            @item.Name
            </td>
            <td>
            @item.BirthData
            </td>

            }*@
        </tr>
    </tbody>
</table>

<div>
    <a asp-action="Index">Back to List</a>
</div>